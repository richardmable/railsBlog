<h1>Here is your current profile:</h1>
<% @profile.each do |f| %>
<p>First Name: <%= f.fname %></p>
<p>Last Name: <%= f.lname %></p>
<p>Location :<%= f.location %></p>
<p>Age: <%= f.age %></p>
<% end %>

<h2>Update your profile:</h2>
<%= form_for @profileUpdate, :url => { :action => "update"} do |f| %>
<%=f.label :First_Name %>
<%=f.text_field :fname %>
<%=f.label :Last_Name %>
<%=f.text_field :lname %>
<%=f.label :Location %>
<%=f.text_field :location %>
<%=f.label :Age %>
<%=f.text_field :age %>
<%=f.submit :Update %>
<% end %>
